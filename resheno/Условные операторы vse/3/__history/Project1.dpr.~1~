program Project1;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

var x,y:real;


begin
writeln('Введите первое расстояние в км ... ');
readln(x);
writeln('Введите второе расстояние в футах');
readln(y);
x:=x*1000;
y:=y*0.45;

if(x>y) then
begin
writeln('2 расстояние Меньше ','(',y:2:2,')');
end
else begin
  writeln('1 расстояние Меньше ','(',x:2:2,')');
end;


 readln;
end.
